   "source": [
    "def pi_arctan(n):\n",
    "    Pi = 0\n",
    "    i = 1\n",
    "    while i <= n:\n",
    "        Pi = Pi + (-1)**(i+1)/(2*i - 1)\n",
    "        i = i+1\n",
    "    return 4*Pi\n",
    "print(\"The value of pi using arctan is\",pi_arctan(6000000))"
   ]
  }
